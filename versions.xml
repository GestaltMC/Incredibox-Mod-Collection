<!--  
HOW TO USE IT:
• <line> category is used to create a new line (using more than 3 might cause issues)
• name="TEXT" is the name of the icon
• ID="NUMBER" is the ID of the icon
• color="#HEX" is the color of the title and checkmark
• locked="true/false" is the lock status of the icon, if its locked it will be impossible to open
• customName="true/false" if its enabled, it will use name of your version instead of the one from the switch-bt-title-tiny@2x.png
• customIcon="asset-v1/ICON.png" is the path to the icon, removing customIcon will use the default one from switch-bt-visu-1@2x.png
• hidden="true/false" if its enabled, it will hide the icon from the list
 -->

<icons>
  <line>
    <icon name="Celcius" ID="1" color="#0087ff" locked="false" customName="true" customIcon="asset-v1/icon-celicus.png" hidden="false"></icon>
        <icon name="Stardust" ID="2" color="#1f2b48" locked="false" customName="true" customIcon="asset-v2/icon-stdustv5.png" hidden="false"></icon>
        <icon name="Dreamland" ID="3" color="#4dc3ff" locked="false" customName="true" customIcon="asset-v3/icon_dreamland.png" hidden="false"></icon>
        <icon name="Travis" ID="4" color="#0086d1" locked="false" customName="true" customIcon="asset-v4/icon-travis.png" hidden="false"></icon>
  </line>
 <line>
  <icon name="Mechanic" ID="5" color="#666" locked="false" customName="true" customIcon="asset-v5/icon-clbx-mechanic.png" hidden="false"></icon>
   <icon name="Locked" ID="6" color="#666" locked="true" customName="true" customIcon="asset-v1/icon.png" hidden="false"></icon>
   <icon name="Locked" ID="7" color="#666" locked="true" customName="true" customIcon="asset-v1/icon.png" hidden="false"></icon>
   <icon name="Locked" ID="8" color="#666" locked="true" customName="true" customIcon="asset-v1/icon.png" hidden="false"></icon>
</line>
</icons>
